    "import matplotlib.pyplot as plt\n",
    "from wordcloud import WordCloud\n",
    "path = '剑南诗稿jieba分词文本.txt'\n",
    "file = open(path, 'r')\n",
    "text = file.read()\n",
    "wordcloud = WordCloud().generate(text)\n",
    "%pylab inline\n",
    "plt.imshow(wordcloud, interpolation='bilinear')\n",
    "plt.axis(\"off\")\n"
   ]
  }
